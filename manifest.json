{
    "name": "clarisse",
    "description": "Declutters the web for you",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ]
}